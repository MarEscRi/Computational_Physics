import math
import pylab as plt
def f(x):
    return math.e**(-x)

q = int(input("How many slices do you want?"))
T = [0]*(q)
S = [0]*(q)
eT = [0]*(q)
eS = [0]*(q)
leT = [0]*(q)
leS = [0]*(q)

N = [0]*(q)


for n in range(1,q):
    a=0
    b=1 
    h=(b-a)/n
    N[n] = n
        #Exact value 
    I = 1 - (math.e)**(-1)

        #Trapeziod
    T[n] = 0.5*f(a) + 0.5*f(b)
    for k in range(1,n):
        T[n] += f(a + k*h)
    T[n] = T[n]*h

        #Simpson 
    S[n]=f(a)+f(b)
    for k in range(1,n,2):
        S[n] +=4*f(a+k*h)
    for k in range(2,n,2):
        S[n] +=2*f(a+k*h)
    S[n] = S[n]*(h/3)

    #error Trapeziod 
    eT[n] = abs((I-T[n])/I)
    #error Simpson
    eS[n] = abs((I-S[n])/I)
    leT[n] = math.log10(eT[n])
    leS[n] = math.log10(eS[n])

print("The exact value is:", I)
print("The Trapezoid rule provides a value of",T[-1])
print("Simpsons rule provides a value of",S[-1])
print("The relative error in trapeziod is:", eT[-1])
print("The relative error in simpson is:", eS[-1])













