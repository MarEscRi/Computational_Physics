import numpy as np
from pylab import plot,show,xlim

data = np.loadtxt("sunspots.txt",float)

x = data[:,0]
y = data[:,1]

Y= [0]*len(y)

k = 6
while k < len(y) - 5: 
    Y[k] = sum(y[k-5:k+6]) 
    Y[k] = Y[k] / 10
    k += 1
    
xlim(5,1000)
plot(x,y,label="Data")
plot(x,Y,label="Average")
plt.legend()
show()

